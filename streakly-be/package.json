{
  "name": "streakly-be",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "watch": "ts-node-dev --respawn src/server.ts",
    "build": "tsc",
    "dev": "ts-node-dev -r tsconfig-paths/register src/server.ts",
    "copy": "copy-folder templates dist/templates",
    "prisma-gen": "npx prisma generate --schema ./prisma",
    "prisma-sync": "npx prisma db push",
    "prisma-reset": "npx prisma migrate reset --schema ./prisma",
    "prisma-migrate": "npx prisma migrate dev --schema ./prisma --name Prisma_Migration"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "prisma": "^6.19.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
